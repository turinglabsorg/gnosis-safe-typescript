{
  "name": "gnosis-safe-typescript",
  "version": "1.0.0",
  "description": "Gnosis safe tests",
  "scripts": {
    "dev:create": "nodemon --watch \"src/*\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node-esm --experimental-specifier-resolution=node src/create-safe.ts\"",
    "dev:send:funds": "nodemon --watch \"src/*\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node-esm --experimental-specifier-resolution=node src/send-funds.ts\"",
    "dev:send:nfts": "nodemon --watch \"src/*\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node-esm --experimental-specifier-resolution=node src/send-nfts.ts\""
  },
  "author": "turinglabs",
  "license": "MIT",
  "dependencies": {
    "@gnosis.pm/safe-core-sdk": "^3.1.1",
    "@gnosis.pm/safe-ethers-lib": "^1.6.1",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.2",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
